java=`which java`
if [ "_$JAVA_HOME" != "_" ]; then
    java=$JAVA_HOME/bin/java
fi
echo "java = $java"


strset="en-uk"
. ../../../res/strings/$strset
if [ -f ../../../res/strings/${strset}.ovr ]; then
    echo "found override string set"
    . ../../../res/strings/${strset}.ovr
fi

dbg=$str29
logs=$str33
us=$str3

echo "================================================================bouncy"
echo "testing $us-sdk_bc.jar"
if [[ -f $us-sdk_bc.jar ]]; then
    $java -ea -cp $us-sdk-test_bc.jar:libs_bouncy/*:$us-sdk_bc.jar:. main
fi

